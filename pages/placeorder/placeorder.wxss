.jfgz .jfbuttom{
  line-height:40px;
font-size:14px;
color:#00cd85;
}
.received_address_content .boxtitle{
  display: flex;
  width: 100%;
  justify-content: center;
  height: 36px;
  align-items: center;
  background-color: #fafafa;
}
.received_address_content .boxtitle text{
  font-size: 14px;
  color: #333;
}
.received_address_content .boxbottom{
  display: flex;
  width: 100%;
  justify-content: center;
  height: 36px;
  align-items: center;
  background-color: #fafafa;
  position: fixed;
  bottom: 0;
}
.received_address_content .boxbottom text{
  font-size: 14px;
  color: #333;
}
.received_address_name .navaA{
  color: #ff6e6e!important;
}