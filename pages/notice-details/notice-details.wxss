@import "../../wxParse/wxParse.wxss";

page{
  background-color: #fff;
}

.noticedetails_content{
  display: flex;
  flex-direction: column;
}

.noticedetails_title_content{
  padding:22rpx 20rpx;
}

.noticedetails_title{
  font-size:16px;
  color:#333333;
  font-weight:bold;
}

.noticedetails_time{
  font-size:10px;
  color:#999999;
  text-align:right;
  margin-right:10px;
}

.noticedetails_box{
  padding: 0 20rpx 10rpx;
}

.noticedetails_box text{
  padding: 10rpx 0;
  font-size: 27rpx;
  color: #666666;
  display: block;
}

.noticedetails_box image{
  max-width: 100%;
}











