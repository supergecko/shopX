.order_state_content {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 32rpx;
  background-color: #fff;
}

.order_state_handle {
  width: 60rpx;
  height: 18rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-top: 16rpx;
}

.order_state_handle::before,
.order_state_handle::after {
  content: '';
  width: 100%;
  height: 3px;
  display: block;
  border-radius: 10px;
  background-color: #e8e8e8;
}

.order_state_box {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 32rpx 0 0;
}

.order_state_box text {
  height: 18px;
  line-height: 18px;
  font-size: 18px;
  color: #333;
  font-weight: bold;
}

.order_state_box image {
  width: 14rpx;
  height: 24rpx;
  margin-left: 24rpx;
}

.order_state_tips {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 32rpx 0 0;
}

.order_state_tips text {
  font-size: 11px;
  color: #666;
  display: block;
}

.order_state_btn {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 32rpx 0 0;
}

.order_state_btn button {
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  margin: 0 10rpx;
  font-size: 13px;
  border-radius: 3px;
  background-color: #fff;
}

.orderdetails_content {
  display: flex;
  flex-direction: column;
  padding: 0 3%;
  background-color: #fff;
  width: 94%;
}

.orderdetails_title_content {
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #f0f0f0;
}

.orderdetails_title_left text {
  font-size: 15px;
  color: #333;
  font-weight: bold;
}

.orderdetails_title_right {
  flex-grow: 0;
  flex-shrink: 0;
}

.orderdetails_title_right image {
  width: 10rpx;
  height: 20rpx;
}

.orderdetails_goods_list_content {
  display: flex;
  flex-direction: column;
}

.orderdetails_goods_list_box {
  display: flex;
  flex-direction: row;
  align-content: center;
  padding: 20rpx 0;
}

.orderdetails_goods_list_left,
.orderdetails_goods_list_left image {
  width: 110rpx;
  height: 110rpx;
  flex-grow: 0;
  flex-shrink: 0;
  margin-right: 20rpx;
  border-radius: 6rpx;
}

.orderdetails_goods_list_right {
  width: 10px;
  height: 110rpx;
  flex-grow: 1;
  flex-shrink: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.orderdetails_goods_list_top,
.orderdetails_goods_list_center,
.orderdetails_goods_list_bottom {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.orderdetails_goods_list_top text {
  font-size: 14px;
  color: #333;
}

.orderdetails_goods_list_top text:nth-child(1) {
  width: 10px;
  flex-grow: 1;
  flex-shrink: 1;
  padding-right: 10px;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.orderdetails_goods_list_center text,
.orderdetails_goods_list_bottom text {
  font-size: 12px;
  color: #999;
}

.orderdetails_goods_list_center text:nth-child(2) {
  text-decoration: line-through;
}

.orderdetails_discountlist_content {
  display: flex;
  flex-direction: column;
  padding: 8rpx 0;
  border-top: 1px dotted #f0f0f0;
}

.orderdetails_discountlist_box {
  padding: 8rpx 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.orderdetails_discountlist_left {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.orderdetails_discountlist_left image {
  width: 30rpx;
  height: 30rpx;
  flex-grow: 0;
  flex-shrink: 0;
  margin-right: 10rpx;
}

.orderdetails_discountlist_right {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.orderdetails_discountlist_left text,
.orderdetails_discountlist_right text {
  font-size: 14px;
  color: #333;
}

.orderdetails_discountlist_right text.red {
  color: #ff0000;
}

.orderdetails_list_content {
  display: flex;
  flex-direction: column;
}

.orderdetails_list_box {
  min-height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 0;
  border-bottom: 1px solid #f0f0f0;
  box-sizing: border-box;
}

.orderdetails_list_box:last-child {
  border-bottom: none;
}

.orderdetails_list_left {
  flex-grow: 0;
  flex-shrink: 0;
  padding: 0 10px 0 0;
}

.orderdetails_list_left text {
  font-size: 14px;
  color: #999;
  display: block;
}

.orderdetails_list_right {
  flex-grow: 1;
  flex-shrink: 1;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.orderdetails_list_right text {
  font-size: 14px;
  color: #333;
  word-break: break-all;
}





.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9;
  color: #fff;
}

.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  background: #fff;
  height: 370px;
}
.commodity_attr_box{
  text-align: center;
}

.order_statusinfo_title{
  height:49px;
  line-height:50px;
  border-bottom:1px solid #f5f5f5;
}
.order_statusinfo_title text{
  font-size:16px;
}

.order_statusinfo_content{
  display:flex;
  height:46px;
  align-items: center;
  position: relative;
}
.order_statusinfo_content .right_content{
  display: flex;
  width: 88%;
 justify-content: space-between;

}
.connectline{
  position: absolute;
  height: 46px;
  width: 1px;
  left: 7%;
  background-color: #eaeaea;
  top: 23px;
}
.order_status_check_one{
  margin-left:6%;
  width:8px;
  height:8px;
  background-color:#eaeaea;
  border-radius:6px;
}
.order_status_check_onenav{
  width:8px;
  height:8px;
  background-color:#4BC877;
  margin-top:17px;
  border-radius:6px;
  margin-left:6%;
  transform: scale(1.5);
 transform-origin: bottom center;
}
.order_status_content{
  margin-left:20px;
  font-size:14px;
}
.order_status_time{
  margin-left:25%;
  font-size:14px;
}
.order_statusinfo_button{
  position:fixed; 
  bottom:0;
  width:100%;
}
.order_statusinfo_button button{
  color:#fff;
  border-radius:0px;
  font-size:15px;
  height:50px;
  line-height:50px;
}

.showmore{
  text-align: center;
  height: 60rpx;
}
.showmore view{
   display: flex;
   width: 24%;
   border-radius: 3px;
   border: 1px solid #ccc;
   padding: 0 6rpx;
   font-size: 12px;
   height:20px;
   line-height:20px;
   margin: auto;
}
.showmoretext{
  margin-left: 10px;
}
.showmorearrowdown{
  width:0px;
  height:0px;
  border-left:8rpx solid transparent;
  border-top:8rpx solid #666;
  border-right:8rpx solid transparent;
   margin: 8px;
}
.showmorearrowup{
  width:0px;
  height:0px;
  border-left:8rpx solid transparent;
  border-bottom:8rpx solid #666;
  border-right:8rpx solid transparent;
  margin: 8px;
}
.orderdetails_psyinfo{
  display:flex;
  background-color:#fff;
  flex-direction:row;
  align-content:center;
  margin-top:1px;
  height:50px;
  line-height:50px;
}
.psylogo image{
  width:35px;
  height:35px;
  margin-left:10px;
}
.psyname{
  margin-left:10px;
  width:55%;
  font-size:14px;
  font-weight:800;
}
.psyphone{
  margin-left:20%;
}