
.hide{
  display:none;
}
.history_search_content{
  padding:10px 3%;
  display: flex;
  flex-direction: row;
  align-items: center;
  width:94%;
}
.channel_head_search_box {
  width: 100%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 20rpx;
  background-color: #f1f1f5;
  border-radius: 4rpx;
}

.channel_head_search_box image {
  width: 32rpx;
  height: 32rpx;
  flex-grow: 0;
  flex-shrink: 0;
  margin-right: 12rpx;
}

.channel_head_search_box text {
  font-size: 12px;
  color: #999;
}

.home_shops_content{
  display:flex;
  flex-direction:column;
  background-color:#f5f5f5;
}
.ordeing_left{
  width: 168rpx; 
  flex-grow:0;
  flex-shrink:0;
  background-color:#fff;
  overflow:hidden;
}
.ordeing_left_list{
  min-height:112rpx;
  display:flex;
  flex-wrap:wrap;
  align-items:center;
  justify-content:center;
  flex-direction:column;
}
.ordeing_left_listnavaA{
  min-height:112rpx;
  display:flex;
  background-color: #f5f5f5;
  flex-wrap:wrap;
  align-items:center;
  justify-content:center;
  flex-direction:column;
}
.ordeing_left_list text{
  font-size:30rpx;
  color:#666666;
}
.ordeing_left_listnavaA text{
  font-size:30rpx;
  color:#00cd85;
}
.ordeing_left_list text:nth-child(2){
  min-width: 34px;
  height: 15px;
  line-height: 15px;
  padding: 0 5px;
  margin-top: 5px;
  text-align: center;
  font-size: 11px;
  color: #999;
  background-color: #f2f2f2;
  border-radius: 100px;
  box-sizing: border-box;
}
.ordeing_left_listnavaA text:nth-child(2){
  min-width: 34px;
  height: 15px;
  line-height: 15px;
  padding: 0 5px;
  margin-top: 5px;
  text-align: center;
  font-size: 11px;
  color: #00cd85;
  background-color: #fff;
  border-radius: 100px;
  box-sizing: border-box;
}
.ordeing_right {
  height: 100%;
  
  flex-grow:1;
  display:flex;
  flex-direction:column;
  background:#f5f5f5;
}
.market_class_box{
  padding: 0 10px 0 5px;
  background:#f5f5f5;
}
.ordeing_right_title{
  height: 92rpx;
  background:#f5f5f5;
  line-height:92rpx;
  display:flex;
}

.ordeing_right_title_left{
  text-align:left;
  width:49%;
}
.ordeing_right_title_left text{
  font-size:30rpx;
  color:#333;
  margin-left:15px;
}
.ordeing_right_title_right{
  text-align:right;
  width:49%;
}
.ordeing_right_title_right text{
  font-size:24rpx;
  color:#999;
  margin-left:15px;
}
.ordeing_right_typelist{
  background:#fff;
  border-radius:4px;
  display:flex;
  flex-wrap:wrap;
  flex-direction:row;
}
.market_class{
  padding:16px;
  flex-direction:column;
  display:flex;
  align-items:center;
  justify-content:center;
}
.market_class text{
  font-size:24rpx;
  color:#333;
}
.market_class text:nth-child(3){
  min-width:34px;
  height:15px;
  line-height:15px;
  padding:0 5px;
  margin-top:5px;
  text-align:center;
  font-size:11px;
  color:#999;
  background-color:#f2f2f2;
  border-radius:100px;
  box-sizing:border-box;
}