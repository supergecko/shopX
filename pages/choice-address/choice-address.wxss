.default_content{
  display: flex;
  flex-direction: column;
  width: 100%;
  margin-bottom: 36px;
}
.choice_head_content{
  height: 88rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.choice_head_text{
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-right: 1px solid #fff;
  box-sizing: border-box;
}

.choice_head_text text{
  max-width: 109rpx;
  flex-grow: 0;
  flex-shrink: 0;
  font-size: 28rpx;
  color: #fff;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.choice_head_text image{
  width: 19rpx;
  height: 10rpx;
  margin-left: 14rpx;
  flex-grow: 0;
  flex-shrink: 0;
}

.choice_head_input{
  flex-grow: 1;
  padding: 0 20rpx;
}

.choice_head_input input{
  width: 81%;
  color: #fff;
}

.choice_head_input image{
  width:16px;
  height:16px;
  position:absolute;
  right:70px;
  top:14px;
}

.choice_head_button{
  padding-right: 20rpx;
}

.choice_head_button button{
  width: 100rpx;
  height: 56rpx;
  line-height: 56rpx;
  font-size: 24rpx;
  padding: 0;
  background-color: #fff;
}

.already_content{
  height: 1030rpx;
  background-color: #fff;
}

.already_title{
  padding: 32rpx 20rpx;
  font-size: 26rpx;
  color: #333;
}

.already_text{
  padding-left: 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.already_text text{
  width:162rpx;
  height:60rpx;
  margin-right:20rpx;
  margin-bottom:16rpx;
  display:flex;
  flex-direction:row;
  text-align:center;
  justify-content:center;
  font-size:24rpx;
  color:#333333;
  flex-grow:0;
  flex-shrink:0;
  border-width:1px;
  border-style:solid;
  border-radius:4rpx;
  border-color:#e6e6e6;
  box-sizing:border-box;
  overflow:hidden;
  line-height:60rpx;
}

.already_text .navaA{
  color:#00cd85;
  border-color:#00cd85;
}

.default_position{
  height: 82rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
  width: 96%;
  padding: 0 2%;
}

.default_position image{
  width: 28rpx;
  height: 28rpx;
  margin-right: 14rpx;
  flex-grow: 0;
  flex-shrink: 0;
}

.default_position text{
  font-size: 24rpx;
}

.default_address_title{
  height: 74rpx;
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #fff;
}

.default_address_title image{
  width: 23rpx;
  height: 21rpx;
  margin-right: 14rpx;
  flex-grow: 0;
  flex-shrink: 0;
}

.default_address_title text{
  font-size: 24rpx;
  color: #999999;
}

.default_address_content{
  background-color: #fff;
}

.default_address_box{
  padding: 10px 20rpx;
  display: flex;
  flex-direction: column;
  position: relative;
}

.default_add{
  font-size: 29rpx;
  color: #333333;
}

.default_info{
  margin-top: 8rpx;
  font-size: 25rpx;
  color: #999999;
}

.default_info text{
  margin-left: 54rpx;
  color: #fff;
}
.adrbq{
  position: absolute;
  top: 10px;
  right: 10px;
  color: #fff;
  height: 18px;
  line-height: 16px;
  text-align: center;
  border-radius: 4px;
  font-size: 12px;
  width: 36px;
}
.adrbq.green{
  background-color: #00cd85;
}
.adrbq.blue{
  background-color: #49aff7;
}
.adrbq.orange{
  background-color: #fe9900;
}
.adrmorebox{
  display: flex;
  width: 96%;
  padding:0 2%;
  font-size:12px;
  align-items:center;
  height: 30px;
}
.adrmorebox image{
  height: 10px;
  width: 12px; 
  margin-left: 5px;
}
.addbox{
  display: flex;
  position: fixed;
  bottom:0;
  width: 100%;
  background-color: #00cd85;
  height: 36px;
  line-height: 36px;
  color: #fff;
  justify-content: center;
  font-size: 14px;
}
