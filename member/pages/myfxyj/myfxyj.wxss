.commisionmy_head_content,
.commisionmy_head_top,
.commisionmy_head_bottom{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.commisionmy_head_content{
  padding: 10px 0 20px;
  background-image: url("data:image/png;base64,");
  background-position: left bottom;
  background-repeat: no-repeat;
  background-size: 100%;
}

.commisionmy_head_top,
.commisionmy_head_bottom{
  padding: 10px 0;
}

.commisionmy_head_top text:nth-child(1){
  font-size: 12px;
  color: #fff;
  display: block;
}

.commisionmy_head_top text:nth-child(2){
  margin-top: 4px;
  font-size: 45px;
  color: #fff;
  display: block;
}

.commisionmy_head_bottom text:nth-child(1){
  font-size: 11px;
  color: #fff;
  display: block;
}

.commisionmy_head_bottom text:nth-child(2){
  margin-top: 4px;
  font-size: 24px;
  color: #fff;
  display: block;
}

.commisionhome_putforward_content{
  padding: 5px 0;
  display: flex;
  flex-direction: row;
  background-color: #fff;
}

.commisionhome_putforward_box{
  width: 50%;
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  border-right: 1px solid #f0f0f0;
  box-sizing: border-box;
}

.commisionhome_putforward_box:last-child{
  border-right: none;
}

.commisionhome_putforward_box text:nth-child(1){
  font-size: 11px;
  color: #808080;
  display: block;
}

.commisionhome_putforward_box text:nth-child(2){
  margin-top: 4px;
  font-size: 18px;
  color: #333;
  display: block;
}

.commisionhome_list_content{
  display: flex;
  flex-direction: column;
  background-color: #fff;
}

.commisionhome_list_box{
  padding: 10px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.commisionhome_list_left{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.commisionhome_list_left image{
  width: 18px;
  height: 18px;
  margin-right: 10px;
  flex-grow: 0;
  flex-shrink: 0;
  border-radius: 50%;
}

.commisionhome_list_left text{
  font-size: 14px;
  color: #333;
  display: block;
}

.commisionhome_list_right image{
  width: 7px;
  height: 12px;
  flex-grow: 0;
  flex-shrink: 0;
  display: block;
}

.commisionhome_btn_content{
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
}

.commisionhome_btn_box{
  padding: 0 10px 10px 10px;
  background-color: #f5f5f5;
}

.commisionhome_btn_box button{
  height: 40px;
  line-height: 40px;
  font-size: 15px;
  color: #fff;
}





