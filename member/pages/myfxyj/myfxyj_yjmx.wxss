
.commisiondetail_head_content{
  padding: 6px 10px;
}

.commisiondetail_head_box{
  padding: 0 10px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #fff;
  border-radius: 4px;
}

.commisiondetail_head_inp{
  padding: 0 10px;
  flex-grow: 1;
  flex-shrink: 1;
}

.commisiondetail_head_inp input{
  height: 32px;
  font-size: 14px;
  color: #333;
}

.commisiondetail_head_box text{
  width: 14px;
  height: 14px;
  line-height: 11px;
  text-align: center;
  font-size: 12px;
  color: #fff;
  display: block;
  background-color: #b3b3b3;
  border-radius: 50%;
}

.commisiondetail_list_content{
  display: flex;
  flex-direction: column;
}

.commisiondetail_list_box{
  display: flex;
  flex-direction: column;
  background-color: #fff;
}

.commisiondetail_list_head{
  padding: 10px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.commisiondetail_list_head text{
  color: #333;
  display: block;
}

.commisiondetail_list_head text:nth-child(1){
  font-size: 14px;
  color: #333;
}

.commisiondetail_list_head text:nth-child(2){
  font-size: 15px;
  color: #333;
}

.commisiondetail_list_main{
  padding: 6px 0;
  display: flex;
  flex-direction: column;
}

.commisiondetail_list{
  padding: 4px 10px;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.commisiondetail_list_left{
  min-width: 60px;
  flex-grow: 0;
  flex-shrink: 0;
}

.commisiondetail_list_left text,
.commisiondetail_list_right text:nth-child(2){
  font-size: 12px;
  color: #999;
}

.commisiondetail_list_right{
  flex-grow: 1;
  flex-shrink: 1;
}

.commisiondetail_list_right text:nth-child(1){
  font-size: 12px;
  color: #333;
}
.loadingmore{
   height:50rpx;
  line-height:50rpx;
  flex-grow:0;
  flex-shrink:0;
  font-size:13px;
  text-align:center;
  background-color:#fff;
  margin:10px 0;
  }
  .nodata{
  display:flex;
  flex-direction:column;
  text-align:center;
  margin:100px;
}
.nodata image{
  width:80px;
  height:80px;
  margin:20px auto;
}
.nodata text{
  color:#bbb;
}
.mask{
  width:100%;
  height:100%;
  background:rgba(0, 0, 0, .4);
  position:fixed;
  top:0;
  left:0;
  z-index:997;
}